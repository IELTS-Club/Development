        <form action="/panel/class-registration" method="POST" >
        <div class="class-registration-grid">

          <div class="class-registration-form">

            <div class="coursename-box">
              
              <label for="" class="icdrn-label">
                نام دوره
              </label>

              <div class="select">
                <select id="standard-select" name="className">
                  <option value="Ielts">کلاس آنلاین آموزشی آیلتس</option>
                  <option value="Toefl">
                    کلاس آنلاین آموزشی تاقل
                  </option>
                  <option value="Emigration">
                    کلاس آنلاین آموزشی مهاجرت
                  </option>
                  <option value="Conversation">
                    کلاس آنلاین آموزشی مکالمه
                  </option>
                  <option value="PTE">
                    کلاس آنلاین آموزشی PTE
                  </option>
                  <option value="Duolingo">
                    کلاس آموزشی آنلاین دولینگو
