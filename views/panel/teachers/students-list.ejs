              <th>شماره</th>
              <th>نام دانشجو</th>
              <th>دوره</th>
              <th>نوع دوره</th>
              <th>مدرس</th>
              <th>تعیین سطح</th>
              <th>شماره تماس</th>
              <th>ایمیل</th>
              <th>توضیحات</th>
            </tr>

            <%
            if(students.length > 0){ %>
           <% let i=0  %>
           <%    students.forEach(element => { 
            function selectClassName(){
              if(element.className=="Ielts")
              return  " آیلتس";
            
            if(element.className=="Toefl")
              return " تافل"
            
            if(element.className=="Duolingo")
              return " Duolingo"
            
            if(element.className=="Emigration")
              return " مهاجرت"
            
            if(element.className=="Conversation")
              return " مکالمه"
            }    
            function selectClassType(){
              if(element.classType=="Private")
              return  "خصوصی";
            
            if(element.classType=="Public")
              return "عمومی"
            }
            function selectClassTeacher(){
              if(element.classTeacher=="Noorzad")
              return  "دکتر پزمان نورزاد";
            
            if(element.classTeacher=="Bahrami")
              return "خانم هانیه بهرامی"
            
            if(element.classTeacher=="Rafiea")
              return "خانم زهرا رفیعی"
            
            if(element.classTeacher=="Nouri")
              return "خانم مینا نوری"
            
            if(element.classTeacher=="Zolfaghari")
              return "خانم آرزو ‌‌ذولفقاری"
            
            if(element.classTeacher=="Jadidi")
            return "اقای جواد جدیدی"
            }
            if(element.classDeatails==""){
              element.classDeatails="-"
            }
            
            
            %>  
           <%     i++; %>
              
             
              <tr>
              <td><%= i %></td>
              <td><%= element.name.name %></td>
              <td><%= selectClassName(); %></td>
              <td><%= selectClassType(); %></td>
              <td><%= selectClassTeacher(); %></td>
              <td><%= element.quizScore.level %></td>
              <td><%= element.name.phone %></td>
              <td><%= element.name.email %></td>
              <td><%= element.classDeatails %></td>
       
            </tr>

         <%  }); %>
