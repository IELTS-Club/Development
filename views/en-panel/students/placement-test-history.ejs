      <div class="placementtest-history">

        <div class="placementtest-history-title">
          <h2>
            <span>
              |
            </span>
            Placement Test History
          </h2>
          <button type="submit" class="icdrn-btn"  id="startPlacementTestBtn">

            New Placement Test 

            <span>+</span>

          </button>
        </div>

        <div class="placementtest-history-table-box">

          <table class="placementtest-history-table">
            <tr>
              <th>id</th>
              <th>user name</th>
              <th>correct answer</th>
              <th>false answer</th>
              <th>student level</th>
              <th>quiz date</th>
              <th>quiz type</th>
              <th>Payment status</th>
            </tr>
          
          <%   if(quizes.length!= 0){ %> 
            <% let i=0; %>
           <%    quizes.forEach(element => { %>
            <%     i++; 
              function trueAnswer(){
              if(element.trueAnswer==null){
               return  "-"
              }return element.trueAnswer;
            }
            function falseAnswer(){
              if(element.falseAnswer==null){
               return  "-"
              }return element.falseAnswer;
            }
            function level(){
              if(element.level==null){
               return  "-"
              }return element.level;
            }
            %> 

            <tr>
              <td><%= i %></td>
              <td><%= element.name %></td>
              <td><%= trueAnswer(); %></td>
              <td><%= falseAnswer(); %></td>
              <td><%= level(); %></td>
              <td><%= element.Date %></td>
              <%function showType() {
              if(element.quizType=="Written"){
                return "Written"
              }else{
                return "Oral"
              }} %>
              <td><%= showType(); %> </td>
              <td>
                <span>paied</span>
              </td>
            </tr>
         <%              
            });
          }  %>

          </table>

        </div>

      </div>

