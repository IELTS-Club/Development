    <div class="main">

      <div class="placementtest-history">

        <div class="placementtest-history-title">
          <h2>
            <span>
              |
            </span>
            Placements History
          </h2>
          <button type="submit" class="icdrn-btn">
       
            New Test

            <span>+</span>

          </button>
        </div>

        <div class="placementtest-history-table-box">

          <table class="placementtest-history-table">

            <tr>
              <th>Row</th>
              <th>Fullname</th>
              <th>Correct Answer</th>
              <th>Wrong Answer</th>
              <th>Student's Level</th>
              <th>Test Type</th>
              <th>Result Control</th>
              <th>Date</th>
              <th>Payment Status</th>
              <th>Phone Number</th>
            </tr>


            <%   if(quizes.length != 0){ %> 
              <% let i=0; %>
             <%    quizes.forEach(element => { %>
              <%     i++; 
              
             function selectTrueAnswer(){
               if(element.trueAnswer==null){
                 return "-"
               }
               return element.trueAnswer;
             }
             function selectFalseAnswer(){
               if(element.falseAnswer==null){
                 return "-"
               }
               return element.falseAnswer
             }
             function selectLevel(){
               if(element.level==null){
                 return "-"
               }
               return element.level
             }
             function showType() {
              if(element.quizType=="Written"){
                return "Written"
              }else{
                return "Oral"
              }}
              
              %> 
              

            <tr>
              <td><%= i %></td>
                <td><%= element.name %></td>
                
                <td><%= selectTrueAnswer();  %> </td>
                <td><%= selectFalseAnswer(); %></td>
                <td><%= selectLevel(); %></td>
                <td><%= showType(); %></td>
              <td>
                <span>
                  <% if(element.quizType=="Oral"){ %>
                    <form action="/en/teachers/placement-test-list/<%= element._id %>" method="GET">
                    <button type="submit" style="outline: 0; border:0; cursor:pointer;" id="reportTest">Submit Score</button></form> 
                 <% }else{ %> 
                 
                  <button  type="submit" style="outline: 0; border:0; "> Automatic</button>
                  <% } %>  
                </span>
            </td>
              <td><%= element.Date %></td>
              <td>
                <span>Paid</span>
              </td>
              <td><%= element.phone %></td>
            </tr>


            <%              
          });
        }  %>
          </table>

        </div>

