  <section class="contact container">

    <div class="contact-grid">

      <div class="contact-form">

        <div class="form-inner">
          <form action="">

            <div class="fullname-box">
              <label class="icdrn-label" for="">
                Full Name
              </label>
            <input type="text" class="icdrn-input full-input" placeholder="
            Enter Your Fullname">
            </div>

  
            <div class="row">
              <div class="phone-box">
                <label class="icdrn-label" for="">
                  Phone Number
                </label>
              <input type="number" class="icdrn-input phone-input" placeholder="Enter Your Phone Number">
              </div>
    
             <div class="email-box">
    
              <label class="icdrn-label" for="">
                Email Address
              </label>
              <input type="email" class="icdrn-input email-input" placeholder="
              Enter Your Email Address">
    
             </div>
            </div>
  
  
            <div class="message-box">
              <label class="icdrn-label" for="">
                Your Message
              </label>
            <input type="text" class="icdrn-input full-input" placeholder="
            Enter Your Message">
               </div>
  
            <div class="contact-btn-box">
  
              <button type="submit" class="contact-btn full-btn" id="registerButton">
                Send Message
              </button>
  
            </div>

        </form>

        </div>

      </div>

      <div class="contact-illustration">

        <img src="/en/images/Contact-Illustration.svg" alt="">

        <div class="contact-btn-box">

        <a href="">

          <button type="submit">
            <i class="fas fa-mobile-alt"></i>
            Call
